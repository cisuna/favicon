{
	"name": "cisuna/favicon",
	"description": "A configurable PHP solution to auto generate favicons and HTML meta tags from a original PNG file.",
	"type": "library",
	"keywords": [
		"favicon",
		"generate",
		"png",
		"ico"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Hieu Le",
			"email": "letrunghieu.cse09@gmail.com",
			"homepage": "http://www.hieule.info"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"imagine/imagine": "~0.1",
		"symfony/console": ">=2.0"
	},
	"suggest": {
		"ext-imagick": "To use the binary and generate favicon images"
	},
	"bin": [
		"favicon"
	],
	"config": {
		"bin-dir": "bin"
	},
	"autoload": {
		"psr-4": {
			"HieuLe\\Favicon\\": "src/"
		},
		"files": ["src/helpers.php"]
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0"
	}
}
